{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Current File with .venv",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "python": "${workspaceFolder}/.venv/Scripts/python.exe"
    }
  ]
}
